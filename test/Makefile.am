
include defs.am     # generated by generate_defs.pl called from configure.ac

AM_CPPFLAGS = @GLIB_CFLAGS@ -DTOP_SRCDIR=\"$(abs_top_srcdir)\"

LIBS = @LIBM_LIB@ @PTHREAD_LIBS@ @GLIB_LIBS@ @HAMLIB_LIBS@ \
       @PANEL_LIBS@ @CURSES_LIBS@ @CMOCKA_LIBS@ \
       @PYTHON_LIBS@ \
        -Wl,-wrap=sleep \
        -Wl,-wrap=key_get \
        -Wl,-wrap=key_poll \
        -Wl,-wrap=sendto \
        -Wl,-wrap=wgetch \
        -Wl,-wrap=refreshp

noinst_HEADERS = \
    test.h

AM_CFLAGS += @PTHREAD_CFLAGS@

AM_CPPFLAGS += -DSRCDIR=\"$(abs_srcdir)\"

DATA_FILES = data/ie_empty_call.txt data/ie_long_line.txt \
	     data/ie_no_comma.txt data/ie_ok.txt \
	     data/ie_ok_dos.txt data/ie_ok_multi_column.txt \
	     data/ie_ok_tab.txt \
             data/continents.txt data/countries.txt \
	     generate_defs.pl

EXTRA_DIST = $(DATA_FILES)

clean-local:
	rm -f *test.log
