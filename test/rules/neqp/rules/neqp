CONTEST=neqp
LOGFILE=neqp.log
CONTEST_MODE
CABRILLO=UNIVERSAL

# The custom multiplier list should be stored in the tlf working directory.
# If outside New England, change to neqp_mults_outside.
MULT_LIST=neqp_mults

#
##################################
#                                #
#  Messages F1= to F12=          #
#  Message CQ_TU_MSG=            #
#  Message S&P_TU_MSG=           #
#                                #
#  % = call                      #
#  @ = hiscall                   #
#  # = serial                    #
#  [ = RST                       #
#  + = increase cw speed         #
#  - = decrease cw speed         #
#                                #
##################################
#
F1=CQ % TEST
F2=@ DE %
F3=@ ++5NN-- HMD MA
F4=TU 73
F5=@
F6=%
F7=@ SRI DUPE
F8=HMD MA
F9=?
F10=QRZ?
F11=K
F12=CQ % % ++TEST--
#
CQ_TU_MSG=TU %
S&P_TU_MSG=TU ++5NN-- HMD MA
#
