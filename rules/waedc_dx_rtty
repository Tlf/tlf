CONTEST=waedc
LOGFILE=waedc.log
CONTEST_MODE
CABRILLO=UNIVERSAL
QTC=RECV
CONTINENTLIST=SA,NA,AF,AS,OC
BANDWEIGHT_MULTIS=80:4;40:3;20:2;15:2;10:2
COUNTRY_MULT
EXCLUDE_MULTILIST=CONTINENTLIST
ONE_POINT

#################################
#NOB4
#
##################################
#                                #
#  Messages F1= to F12=          #
#  Message CQ_TU_MSG=            #
#  Message S&P_TU_MSG=           #
#                                #
#  % = call                      #
#  @ = hiscall                   #
#  # = serial                    #
#  [ = RST                       #
#  + = increase cw speed         #
#  - = decrease cw speed         #
#                                #
##################################
#
F1=CQ TEST DE %
F2=@ DE %
F3=CFM ! UR 599 #
F4=TU 73
F5= @
F6=%
F7=@ SRI QSO B4 GL
F8=AGN
F9= ?
F10= QRZ?
F11= PSE K
F12=TEST % %
#
CQ_TU_MSG=TU ! DE % TEST
S&P_TU_MSG=TU ! 599 #
#
#ALT_0=
#ALT_1=
#ALT_2=
#ALT_3=
#ALT_4=
#ALT_5=
#ALT_6=
#ALT_7=
#ALT_8=
#ALT_9=
#

QR_F1=QTC?
QR_F2=QRV
QR_F3=R
QR_F5=TIME?
QR_F6=CALL?
QR_F7=NR?
QR_F8=AGN
QR_F10=QSL ALL

QTCREC_RECORD
QTCREC_RECORD_COMMAND=rec -r 8000 $ -q


